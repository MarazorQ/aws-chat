{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "ValidateBody",
    "required": ["email", "password", "confirmationCode"],
    "type": "object",
    "properties": {
        "email": {
            "type": "string",
            "minLength": 6,
            "maxLength": 127
        },
        "password": {
            "type": "string",
            "minLength": 6,
            "maxLength": 20
        },
        "confirmationCode": {
            "type": "string",
            "minLength": 6,
            "maxLength": 6
        }
    }
  }